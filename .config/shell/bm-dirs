cca ${XDG_CACHE_HOME:-$HOME/.cache}
cf  ${XDG_CONFIG_HOME:-$HOME/.config}
cfv ${XDG_CONFIG_HOME:-$HOME/.config}/nvim
cfl ${XDG_CONFIG_HOME:-$HOME/.config}/lf
cfz ${XDG_CONFIG_HOME:-$HOME/.config}/zsh
cls ${XDG_DATA_HOME:-$HOME/.local/share}
clb $HOME/.local/bin
clc $HOME/.local/src
d   ${XDG_DOWNLOAD_DIR:-$HOME/Downloads}
cm  ${XDG_MUSIC_DIR:-$HOME/Music}
mn  /mnt
pp  ${XDG_PICTURES_DIR:-$HOME/Pictures}
vv  ${XDG_VIDEOS_DIR:-$HOME/Videos}
